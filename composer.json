{
    "name": "laravel-dev-kit/core",
    "description": "Laravel Development Kit",
    "license": "MIT",
    "authors": [
        {
            "name": "walshdev",
            "email": "alexwalsh.dev@gmail.com",
            "homepage": "https://devclouse.com"
        }
    ],
    "homepage": "https://github.com/laravel-dev-kit/core",
    "keywords": ["Laravel", "Core"],
    "require": {
        "illuminate/support": "~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "LaravelDevKit\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelDevKit\\Core\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelDevKit\\Core\\ServiceProvider"
            ],
            "aliases": {
                "Core": "LaravelDevKit\\Core\\Facades\\Core"
            }
        }
    }
}
